{
    "name": "dayplanner",
    "version": "0.4.1",
    "private": true,
    "main": "index.html",
    "homepage": "./public",
    "repository": "https://gitlab.com/Kevin.Hahn/dayplanner.git",
    "scripts": {
        "start": "vite",
        "start:exposed": "vite --host",
        "build": "vite build",
        "test": "vitest run --coverage",
        "test:ui": "vitest --ui --coverage",
        "cypress:open": "cypress open",
        "lint": "eslint --max-warnings 0 src",
        "prepare": "husky install",
        "tauri": "tauri",
        "app:build": "tauri build",
        "app:run": "tauri run",
        "release:major": "commit-and-tag-version --release-as major",
        "release:minor": "commit-and-tag-version --release-as minor",
        "release:patch": "commit-and-tag-version --release-as patch",
        "release:candidate:major": "commit-and-tag-version --prerelease rc --release-as major",
        "release:candidate:minor": "commit-and-tag-version --prerelease rc --release-as minor",
        "release:candidate:patch": "commit-and-tag-version --prerelease rc --release-as patch"
    },
